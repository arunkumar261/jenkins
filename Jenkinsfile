pipeline {
    agent any
    stages {
        stage('DEV') {
            steps {
              echo "Hello this is DEV Stage"
            }
        }
        
         stage('QA') {
            steps {
              echo "Hello this is QA Stage"
            }
        }
         stage('PRE-PROD') {
            steps {
              echo "Hello this is PRE-PROD Stage"
            }
        }
         stage('PROD') {
            steps {
              echo "Hello this is PROD Stage"
            }
        }
    }
}
