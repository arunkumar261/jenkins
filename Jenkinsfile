pipeline {
    agent any
    stages {
        stage('DEV') {
            steps {
              echo "Hello this is DEV Stage"
            }
        }
        
         stage('QA-testing') {
            steps {
              echo "Hello this is QA-testing Stage"
            }
        }
        stage('UAT') {
            steps {
              echo "Hello this is UAT Stage"
            }
        }
        stage('SAT') {
            steps {
              echo "Hello this is SAT Stage"
            }
        }
         stage('PRE-PROD') {
            steps {
              echo "Hello this is PRE-PROD Stage"
            }
        }
         stage('PROD') {
            steps {
              echo "Hello this is PROD Stage"
            }
        }
    }
}
